{
  "name": "constantinescu.io",
  "version": "0.0.1",
  "description": "My personal website.",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "eslint": "eslint '*.js' 'pages/**/*.js'",
    "flow": "flow",
    "lint-staged": "lint-staged",
    "lint": "run-p eslint 'flow -- check'",
    "precommit": "npm run lint-staged",
    "prettier-eslint": "prettier-eslint --write 'pages/**/*.js'",
    "start": "next start",
    "test": "npm run lint"
  },
  "repository": "git@github.com:andreiconstantinescu/constantinescu.io.git",
  "author": "Andrei Constantinescu <andrei@constantinescu.io>",
  "license": "MIT",
  "dependencies": {
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.49.1",
    "next": "^3.0.1-beta.10",
    "react": "^16.0.0-alpha.13",
    "react-dom": "^16.0.0-alpha.13"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "eslint": "^4.1.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "prettier-eslint-cli": "^4.1.1"
  },
  "lint-staged": {
    "linters": {
      "**/*.js": ["prettier-eslint --write"]
    }
  }
}
