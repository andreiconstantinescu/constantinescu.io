{
  "name": "constantinescu.io",
  "version": "1.0.0",
  "private": true,
  "description": "My personal website.",
  "scripts": {
    "dev": "node ./server",
    "build": "next build",
    "start": "NODE_ENV=production node ./server",
    "flow": "npx flow",
    "test": "npm run flow",
    "submodule:update": "git submodule update --remote; git submodule foreach -q --recursive 'branch=\"$(git config -f $toplevel/.gitmodules submodule.$name.branch)\"; git checkout $branch'",
    "export": "echo $EXTERNAL_PATH | xargs next export -o"
  },
  "repository": "git@github.com:andreiconstantinescu/constantinescu.io.git",
  "author": "Andrei Constantinescu <andrei@constantinescu.io>",
  "license": "MIT",
  "dependencies": {
    "apollo-boost": "^0.1.10",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "graphql-tag": "^2.9.2",
    "graphql-yoga": "^1.14.12",
    "isomorphic-unfetch": "^2.0.0",
    "lodash": "^4.17.10",
    "markdown-in-js": "^1.1.4",
    "next": "^6.0.4-canary.1",
    "prismic-dom": "^2.1.0",
    "prismic-javascript": "2.0.0-beta.0",
    "react": "^16.4.0",
    "react-apollo": "^2.1.9",
    "react-dom": "^16.4.0",
    "react-markdown": "^3.3.4",
    "turndown": "^4.0.2",
    "unstated": "^2.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "babel-plugin-transform-flow-strip-types": "^7.0.0-beta.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^7.0.0-beta.3",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "flow-bin": "^0.75.0",
    "husky": "^1.0.0-rc.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
