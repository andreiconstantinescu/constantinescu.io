{
  "name": "constantinescu.io",
  "version": "1.0.0",
  "private": true,
  "description": "My personal website.",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "eslint": "eslint '*.js' 'pages/**/*.js'",
    "flow": "flow",
    "flow-coverage": "flow-coverage-report -i 'pages/**/*.js' -t html -o coverage/flow",
    "lint-staged": "lint-staged",
    "lint": "run-p eslint 'flow -- check'",
    "precommit": "npm run lint-staged",
    "prettier-eslint": "prettier-eslint --write 'pages/**/*.js'",
    "start": "next start --port=$PORT",
    "test": "npm run lint"
  },
  "repository": "git@github.com:andreiconstantinescu/constantinescu.io.git",
  "author": "Andrei Constantinescu <andrei@constantinescu.io>",
  "license": "MIT",
  "dependencies": {
    "babel-preset-flow": "^7.0.0-alpha.15",
    "express": "^4.15.3",
    "flow-bin": "^0.51.0",
    "flow-coverage-report": "^0.3.0",
    "next": "beta",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.0.0-alpha.15",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.1.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.1",
    "npm-run-all": "^4.0.2",
    "prettier-eslint-cli": "^4.1.1"
  },
  "lint-staged": {
    "linters": {
      "**/*.js": ["prettier-eslint --write"]
    }
  },
  "engines": {
    "node": "^8.1.4",
    "npm": "^5.3.0",
    "yarn": "^0.28.1"
  }
}
