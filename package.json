{
  "name": "constantinescu.io",
  "version": "1.0.0",
  "private": true,
  "description": "My personal website.",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "flow": "flow",
    "test": "npm run flow",
    "submodule:update":
      "git submodule update --remote; git submodule foreach -q --recursive 'branch=\"$(git config -f $toplevel/.gitmodules submodule.$name.branch)\"; git checkout $branch'",
    "export": "echo $EXTERNAL_PATH | xargs next export -o"
  },
  "repository": "git@github.com:andreiconstantinescu/constantinescu.io.git",
  "author": "Andrei Constantinescu <andrei@constantinescu.io>",
  "license": "MIT",
  "dependencies": {
    "next": "^6.0.4-canary.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-flow-strip-types": "^7.0.0-beta.3",
    "babel-preset-flow": "^7.0.0-beta.3",
    "flow-bin": "^0.74.0",
    "husky": "^1.0.0-rc.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
